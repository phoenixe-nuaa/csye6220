<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.UsersCoursesEntity" table="users_courses" schema="duodb">
        <composite-id>
<!--        <composite-id mapped="true" class="POJO.UsersCoursesEntityPK">-->
            <key-property name="userId">
                <column name="user_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="courseId">
                <column name="course_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>